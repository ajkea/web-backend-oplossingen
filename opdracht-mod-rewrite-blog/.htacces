RewriteEngine On
RewriteRule ^artikels/$ artikel-overzicht.php
RewriteRule ^toevoegen/$ artikel-toevoegen-form.php
RewriteRule ^confirm/$ artikel-toevoegen.php
RewriteRule ^zoeken/content/([^/\.]+)/?$ artikel-zoeken.php?zoeken=$1
RewriteRule ^zoeken/datum/([^/\.]+)/?$ artikel-zoeken.php?jaartal=$1
RewriteRule ^artikels/zoeken/$ artikel-zoeken-redirect.php